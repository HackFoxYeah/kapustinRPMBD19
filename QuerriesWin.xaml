        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:kapustinRPMBD18"
        mc:Ignorable="d"
        Title="QuerriesWin" Height="450" Width="800" ResizeMode="NoResize"
